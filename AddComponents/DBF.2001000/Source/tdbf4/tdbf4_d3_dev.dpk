{$MINENUMSIZE 1}
{$IMAGEBASE $00400000}
{$DESCRIPTION 'TDbf V4 for Delphi 3'}
{$DESIGNONLY}
{$IMPLICITBUILD OFF}

requires
  vcl30,
  VCLDB30;

contains
  UDbfPagedFile,
  dbf_reg,
  UDbfCommon,
  UDbfCursor,
  UDbfEngine,
  UDbfFieldDef,
  UDbfFile,
  UDbfIndex,
  UDbfMemo,
  dbf;

end.
