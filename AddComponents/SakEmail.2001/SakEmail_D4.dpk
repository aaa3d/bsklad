package SakEmail;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'SakEmail by the SakEmail team'}
{$IMPLICITBUILD ON}

requires
  vcl50;

contains
  SakRegister in 'src\SakRegister.pas',
  SakPOP3 in 'src\SakPOP3.pas',
  SakSMTP in 'src\SakSMTP.pas',
  SakMsg in 'src\SakMsg.pas',
  sak_util in 'src\sak_util.pas',
  SakMIME in 'src\SakMIME.pas',
  SakIMAP in 'src\SakIMAP.pas',
  SakAttFile in 'src\SakAttFile.pas';

end.
