{$DESCRIPTION 'FastReport 2.4 ADS Components'}
{$IMPLICITBUILD ON}

requires
  VCL,
  VCLDB,
  VCLSMP,
  FR6,
  ADSD60;

contains
  FR_ADSReg in 'FR_ADSReg.pas',
  FR_ADSDB in 'FR_ADSDB.PAS',
  FR_ADSMd in 'FR_ADSMd.pas',
  FR_ADSQuery in 'FR_ADSQuery.pas',
  FR_ADSQueryParam in 'FR_ADSQueryParam.pas',
  FR_ADSTable in 'FR_ADSTable.pas';

end.
